{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "q",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "q",
            "kebab-case"
        ],
        "curly": false,
        "no-bitwise": false
    }
}